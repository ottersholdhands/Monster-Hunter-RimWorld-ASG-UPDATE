<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef Name="KelbiPart" ParentName="PawnmorphPart" Abstract="true">
		<classInfluence>KelbiMorph</classInfluence>
	</Pawnmorph.Hediffs.MutationDef>
	<Pawnmorph.Hediffs.MutationDef ParentName="KelbiPart">
		<defName>EtherKelbiSnout</defName>
		<label>Kelbi snout</label>
		<description>The lower part of [PAWN_nameDef]'s face has reformed into a deer-like Kelbi snout.</description>
		<initialSeverity>0</initialSeverity>
		<parts>
			<li>Jaw</li>
		</parts>
		<graphics>
			<Muzzle>Parts/Kelbi/Kelbi_Face</Muzzle>
		</graphics>
		<stages>
			<li Class="Pawnmorph.Hediffs.MutationStage">
				<label>new</label>
				<capMods>
					<li>
						<capacity>Talking</capacity>
						<offset>-0.30</offset>
					</li>
					<li>
						<capacity>Eating</capacity>
						<offset>0.10</offset>
					</li>
				</capMods>
			</li>
			<li Class="Pawnmorph.Hediffs.MutationStage">
				<minSeverity>1</minSeverity>
				<label>adapted</label>
				<capMods>
					<li>
						<capacity>Talking</capacity>
						<offset>-0.06</offset>
					</li>
					<li>
						<capacity>Eating</capacity>
						<offset>0.10</offset>
					</li>
				</capMods>
			</li>
		</stages>
		<comps>
			<li Class="HediffCompProperties_VerbGiver">
				<tools>
					<li>
						<capacities>
							<li>Bite</li>
						</capacities>
						<power>8</power>
						<cooldownTime>2.6</cooldownTime>
						<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
						<chanceFactor>0.7</chanceFactor>
					</li>
				</tools>
			</li>
			<li Class="Pawnmorph.Hediffs.CompProperties_MutationSeverityAdjust">
				<severityPerDay>0.25</severityPerDay>
			</li>
		</comps>
	</Pawnmorph.Hediffs.MutationDef>
	<Pawnmorph.Hediffs.MutationDef ParentName="KelbiPart">
		<defName>EtherKelbiHorns</defName>
		<description>A pair of Kelbi horns adorn the top of [PAWN_nameDef]'s head.</description>
		<label>Kelbi horns</label>
		<parts>
			<li>Skull</li>
		</parts>
		<graphics>
			<Horns>
				<path>Parts/None/None</path>
				<genderGraphics>
					<Male>Parts/Kelbi/Kelbi_Horns_Male</Male>
					<Female>Parts/Kelbi/Kelbi_Horns_Female</Female>
				</genderGraphics>
			</Horns>
		</graphics>
		<stages>
			<li Class="Pawnmorph.Hediffs.MutationStage">
				<!--Needs Stats-->
			</li>
		</stages>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef ParentName="KelbiPart">
		<defName>EtherKelbiTail</defName>
		<label>Kelbi tail</label>
		<description>A short, bushy Kelbi tail.</description>
		<parts>
			<li>Tail</li>
			<li>MorphTail</li>
		</parts>
		<graphics>
			<Tail>Parts/Kelbi/Kelbi_Tail</Tail>
		</graphics>
		<stages>
			<li Class="Pawnmorph.Hediffs.MutationStage">
				<statOffsets>
					<PM_BodySize>0.15</PM_BodySize>
				</statOffsets>
			</li>
		</stages>
	</Pawnmorph.Hediffs.MutationDef>
	
	<Pawnmorph.Hediffs.MutationDef ParentName="KelbiPart">
		<defName>EtherKelbiEar</defName>
		<label>Kelbi ear</label>
		<description>[PAWN_nameDef]'s ear has reformed into a cute fuzzy deer-like ear.</description>
		<stages>
			<li Class="Pawnmorph.Hediffs.MutationStage">
				<partEfficiencyOffset>0.1</partEfficiencyOffset>
			</li>
		</stages>
		<graphics>
			<LeftEar>
				<path>Parts/None/None</path>
				<genderGraphics>
					<Male>Parts/Kelbi/Kelbi_Ear_Left_Male</Male>
					<Female>Parts/Kelbi/Kelbi_Ear_Left_Female</Female>
				</genderGraphics>
			</LeftEar>
			<RightEar>
				<path>Parts/None/None</path>
				<genderGraphics>
					<Male>Parts/Kelbi/Kelbi_Ear_Right_Male</Male>
					<Female>Parts/Kelbi/Kelbi_Ear_Right_Female</Female>
				</genderGraphics>
			</RightEar>
		</graphics>
		<parts>
			<li>Ear</li>
		</parts>
	</Pawnmorph.Hediffs.MutationDef>
</Defs>
