<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/MorphDef.xsd">
	<Pawnmorph.MorphDef ParentName="SmallCervidMorph">
		<defName>KelbiMorph</defName>
		<label>Kelbimorph</label>
		<description>A deer-like morph with traits similar to that of a Kelbi.</description>
		<race>Kelbi</race>
		<categories>
			<li>Arid</li>
			<li>Temperate</li>
		</categories>
		<fullTransformation>PawnmorphKelbiTF</fullTransformation>
		<partialTransformation>PawnmorphKelbiTFPartial</partialTransformation>
		<raceSettings>
			<graphicsSettings>
				<skinColorOverride>(61, 99, 98)</skinColorOverride>
				<femaleSkinColorOverride>(69, 96, 109)</femaleSkinColorOverride>
			</graphicsSettings>
		</raceSettings>
		<transformSettings>
			<transformationMemory>KelbiTransformationMemory</transformationMemory>
			<revertedMemory>KelbiRevertedMemory</revertedMemory>
		</transformSettings>
	</Pawnmorph.MorphDef>
</Defs>